<view><block wx:for="{{shopList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="list-item"><image class="img" src="{{item.url}}"></image><view class="message"><view class="name">{{item.name}}</view><view class="rate"><view class="rate-left"><view class="iconfont iconstart-copy"></view><view class="rating">{{item.rating}}</view></view><view class="count">{{item.business_info.recent_order_num_display}}</view></view><view class="price"><view class="fee">{{item.piecewise_agent_fee.description}}</view><view class="nofee">{{item.piecewise_agent_fee.no_subsidy_fee}}</view></view><view class="comment">{{item.promotion_info}}</view><view class="discount"><block wx:for="{{item.business_info.lemon_support_tags}}" wx:for-item="temp" wx:for-index="index" wx:key="index"><block><block wx:if="{{index<5}}"><view class="text">{{temp.text}}</view></block></block></block></view></view></view></block></block></view>